<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.nkl.admin.domain.Score" table="score">
		<id name="score_id" type="java.lang.Integer">
			<column name="score_id" />
			<generator class="native" />
		</id>
		<property name="user_id" type="java.lang.Integer">
			<column name="user_id"  />
		</property>
		<property name="course_id" type="java.lang.Integer">
			<column name="course_id"  />
		</property>
		<property name="score_value" type="java.lang.Double">
			<column name="score_value"  precision="22" scale="0" />
		</property>
		<property name="score_year" type="java.lang.Integer">
			<column name="score_year"  />
		</property>
		<property name="score_year_half" type="java.lang.Integer">
			<column name="score_year_half"  />
		</property>
		<property name="note" type="java.lang.String">
			<column name="note"  length="300" />
		</property>
	</class>
</hibernate-mapping>
